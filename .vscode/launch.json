{
    "version": "0.2.0",
    "configurations": [
        {
            "preLaunchTask": "${defaultBuildTask}",
            "type": "probe-rs-debug",
            "request": "launch",
            "name": "probe_rs Executable Test",
            "cwd": "${workspaceFolder}",
            "program_binary": "target/thumbv6m-none-eabi/debug/papout-psm-rs",
            "chip": "STM32G030F6Px",
            "connect_under_reset": true,
            "speed": 24000,
            "runtimeExecutable": "probe-rs-debugger",
